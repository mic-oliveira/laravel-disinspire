        "michaelferreira/failure-coach": ">=0.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "6.x-dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michaelferreira\\LaravelDisinspire\\Provider\\DisinpireServiceProvider"
