{
    "name": "michaelferreira/laravel-disinspire",
    "description": "Quotes of wisdom from the failure coach",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Michaelferreira\\LaravelDisinspire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Michaelferreira\\LaravelDisinspire\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael de Oliveira Ferreira",
            "email": "oliveiramichael@outlook.com"
        }
